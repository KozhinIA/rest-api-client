@model List<ImportItemLog>
@using RestApiClient.Models
@using Common.Extensions
@{
    int index = 1;
}

<table class="table table-hover table-bordered align-middle">
    <tr>
        <td>№п/п</td>
        <td>Сообщение</td>
    </tr>
    @foreach (var value in Model)
    {
        <tr>
            <td>@(index++)</td>
            <td>@value.Type.GetDisplayValue()</td>
            <td>@value.Message</td>
        </tr>
    }
</table>